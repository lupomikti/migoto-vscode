    "definition": {
      "name": "meta.structure.definition.migoto",
      "match": "(?xi)\n^[ \\t]*\n(?!run|checktextureoverride|(?:exclude_)?preset|handling|clear|special|store|condition|\n  draw(?:indexed|instanced|indexedinstanced|instancedindirect|indexedinstancedindirect)?|dispatch(?:indirect)?|\n  vb\\d|ib|(?:[rf]_)?bb|s?o\\d|od|[vhdgpc]s|\n  (?<!$)[xyzw]\\d{0,3}|\n  this|(?:ini|stereo)params|cursor_(?:mask|color)|\n  (?:(?:BuiltIn)?(?:CommandList|CustomShader)|Resource|ShaderRegex).+\n) # keywords and variables that are not keys and should only be matched by declaration\n\\b(\\w+) # optional whitespace followed by a word-like that is not in the preceding list\n\\s* (=) \\s* # kvp separator surrounded by optional whitespace\n([@\\w\\$.\\-,+/* \\t%=\\\\!><\"\\[\\]';:|&()]+) # partial Rvalue\n(?:$|\\n)",
      "captures": {
        "1": {
          "patterns": [
            {
              "include": "#Lword"
            }
          ]
        },
        "2": {
          "name": "punctuation.separator.key-value.migoto"
        },
        "3": {
          "patterns": [
            {
              "include": "#functional-expression"
            },
            {
              "include": "#literal-list"
            },
            {
              "include": "#constant"
            },
            {
              "include": "#string"
            }
          ]
        }
      }
    },
    "declaration": {
      "name": "meta.structure.declaration.migoto",
      "match": "(?xi)\n^[ \\t]* # optional leading whitespace\n(\n  (?:(?:\\w+)\\ +){0,2}     # between 0 and 2 keywords inclusive\n  (?:[$\\w\\-.\\\\/@& '\\[\\]]+) # another keyword or a variable\n)\n\\s*\n(?:\n  (=) \\s* # assignment/declaration operator, surrounded by optional whitespace\n  ([=$!@\\w.\\-,+/* \\t%\\\\()\"|&><]+) # Rvalue\n)? # Operator and Rvalue are optional, e.g. `global $variable` on a line by itself\n(?:$|\\n)",
      "captures": {
        "1": {
          "patterns": [
            {
              "include": "#Lexpression"
            }
          ]
        },
        "2": {
          "name": "keyword.operator.assignment.migoto"
