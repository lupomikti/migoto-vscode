        {
          "name": "meta.structure.section.texture-override.migoto",
          "begin": "(?i)(?<=^[ \\t]*\\[TextureOverride.+\\][ \\t]*$)",
          "end": "(?i)(?=^\\[.+\\][ \\t]*$|\\z)",
          "patterns": [
